<%
  current_year = Time.current.year
  data_current = @chart_data[current_year] || Array.new(12, 0)
  data_last    = @chart_data[current_year - 1] || Array.new(12, 0)
%>


<script>
  window.chartData = {
    labels: <%= raw @months.to_json %>,
    currentYear: <%= current_year %>,
    lastYear: <%= current_year - 1 %>,
    dataCurrent: <%= raw data_current.to_json %>,
    dataLast: <%= raw data_last.to_json %>
  };
</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.js"></script>
<script
function reloadStudentChart() {
  fetch("/admin/charts/students")
    .then(res => res.json())
    .then(data => {
      window.studentChart.data.datasets[0].data = data.current;
      window.studentChart.data.datasets[1].data = data.last;
      window.studentChart.update();
    });
}
</script>
<script>
document.addEventListener("DOMContentLoaded", function () {
  var data = window.chartData;

  var config = {
    type: "line",
    data: {
      labels: data.labels,
      datasets: [
        {
          label: "Students-" + data.currentYear,
          backgroundColor: "#4c51bf",
          borderColor: "#4c51bf",
          borderWidth: 3,
          data: data.dataCurrent,
          fill: false,
        },
        {
          label: "Students-" + data.lastYear,
          backgroundColor: "#ed64a6",
          borderColor: "#ed64a6",
          borderWidth: 3,
          data: data.dataLast,
          fill: false,
        },
      ],
    },
    options: {
      maintainAspectRatio: false,
      responsive: true,
      title: {
         display: true,
         text: "Student Registrations (" + data.lastYear + " vs " + data.currentYear + ")",
         fontColor: "000",
         fontSize: 16,
       },
      legend: {
        labels: { fontColor: "#000" },
        align: "end",
        position: "bottom",
      },
      scales: {
         xAxes: [
           {
             scaleLabel: {
               display: true,
               labelString: "Month",
               fontColor: "white",
               fontSize: 12,
             },
             ticks: {
               fontColor: "#000",
             },
             gridLines: {
               color: "rgba(0,0,0,0.1)",
             },
           },
         ],
         yAxes: [
            {
              scaleLabel: {
                display: true,
                labelString: "Student Count",
                fontColor: "#000",
                fontSize: 12,
              },
              ticks: {
                beginAtZero: true,
                precision: 0,
                autoSkip: false,
                maxTicksLimit: 10,
                fontColor: "#000",
              },
              gridLines: {
                color: "rgba(0,0,0,0.1)",
              },
            },
          ],
          
       },
       
    },
  };

  var ctx = document.getElementById("line-chart");
  if (ctx) {
    new Chart(ctx.getContext("2d"), config);
  }
});
</script>
<script>
  document.addEventListener("DOMContentLoaded", function() {
    const btn = document.getElementById("avatarMenuButton");
    const dropdown = document.getElementById("avatarDropdown");

    btn.addEventListener("click", function(e) {
      e.stopPropagation();
      dropdown.classList.toggle("hidden");
    });

    document.addEventListener("click", function(e) {
      if (!dropdown.contains(e.target) && !btn.contains(e.target)) {
        dropdown.classList.add("hidden");
      }
    });
  });
</script>


